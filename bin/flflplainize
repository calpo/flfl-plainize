#!/usr/bin/env php
<?php
require_once __DIR__ . '/../vendor/autoload.php';

$option = new \FlflPlainize\Option(\Flflplainize\Option::getopt());

(new \FlflPlainize\FlflPlainize(
    new NoRewindIterator(new SplFileObject('php://stdin')),
    new \FlflPlainize\Outputer()
))->main($option->getKeys());
